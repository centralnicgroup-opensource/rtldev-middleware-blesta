{
  "name": "blesta/ispapi",
  "description": "Ispapi",
  "license": "MIT",
  "type": "blesta-module",
  "require": {
    "blesta/composer-installer": "~1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.3"
  },
  "scripts": {
    "codeCheck": "./vendor/bin/phpcs --standard=PSR2 -q -n --colors apis language config ispapi.php",
    "codeFix": "./vendor/bin/phpcbf --standard=PSR2 -q apis language config ispapi.php"
  }
}
